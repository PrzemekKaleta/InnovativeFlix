<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Nuber_Validation_Of_Episod</fullName>
    <active>true</active>
    <description>Check that Episode number is between 1 and 1 000 000</description>
    <errorConditionFormula>OR(  Episod_Number__c &lt; 1
				,Episod_Number__c &gt; 1000000)</errorConditionFormula>
    <errorDisplayField>Episod_Number__c</errorDisplayField>
    <errorMessage>Episode number should be between 1 and 1.000.000</errorMessage>
</ValidationRule>
