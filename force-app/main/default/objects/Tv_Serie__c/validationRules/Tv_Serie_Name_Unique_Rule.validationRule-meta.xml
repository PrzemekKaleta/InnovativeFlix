<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Tv_Serie_Name_Unique_Rule</fullName>
    <active>true</active>
    <description>Tv Serie must be unique</description>
    <errorConditionFormula>LEN(VLOOKUP($ObjectType.Tv_Serie__c.Fields.Name,
								$ObjectType.Tv_Serie__c.Fields.Name,
								Name))&gt;0</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>Tv Serie of this Name already exist.</errorMessage>
</ValidationRule>
